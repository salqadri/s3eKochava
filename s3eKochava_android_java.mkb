#!/usr/bin/env mkb
# Builder mkb file for the java portion of the s3eKochava extension on android
platform JAVA

files
{
    (source/android)
    s3eKochava.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"
librarypath "incoming/KochavaSDK.jar"

option output-name=lib/android/s3eKochava.jar
